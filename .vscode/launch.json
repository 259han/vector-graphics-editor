{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Qt Application",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/vector-graphics-editor.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
